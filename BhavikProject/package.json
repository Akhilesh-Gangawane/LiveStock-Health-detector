{
  "name": "livestock-illness-react",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:frontend\"",
    "dev:frontend": "vite",
    "dev:api": "cd api && python -m uvicorn main:app --reload --port 8000",
    "build": "vite build",
    "preview": "vite preview",
    "start:mock": "json-server --watch mock/db.json --port 4000",
    "install:api": "cd api && pip install -r requirements.txt"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.76.1",
    "chart.js": "^4.4.0",
    "framer-motion": "^10.16.4",
    "i18next": "^23.1.2",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-audio-voice-recorder": "^2.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^12.3.1",
    "react-media-recorder": "^1.7.2",
    "react-router-dom": "^6.14.1",
    "tailwindcss": "^3.3.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.2",
    "json-server": "^0.17.3",
    "postcss": "^8.4.24",
    "vite": "^5.0.0"
  }
}
